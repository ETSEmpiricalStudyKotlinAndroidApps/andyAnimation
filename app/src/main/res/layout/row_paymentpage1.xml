<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="paymentpage1RowModel" type="com.ankitpatelsandroidapplication.app.modules.paymentpage.data.model.Paymentpage1RowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearForm"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" android:orientation="vertical">
        <TextView android:gravity="top|start" android:id="@+id/txtTitle"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_width="wrap_content"
            android:text="@{paymentpage1RowModel.txtTitle}"
            style="@style/txtSkModernistskmodernistregular12" tools:text="@string/lbl_exp_date"/>
        <TextView android:gravity="start" android:id="@+id/txtForm"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_16pxv"
            android:paddingStart="@dimen/_10pxh"
            android:paddingTop="@dimen/_15pxv"
            android:text="@{paymentpage1RowModel.txtForm}"
            style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_dd_mm"/>
    </LinearLayout>
</layout>
