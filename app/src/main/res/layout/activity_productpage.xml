<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="productpageVM" type="com.ankitpatelsandroidapplication.app.modules.productpage.data.viewmodel.ProductpageVM"/>
    </data>
    <LinearLayout android:id="@+id/linearGroup123"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearGroup105"
                android:layout_gravity="top|start"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:clickable="true"
                    android:id="@+id/linearGroup399"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.4"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylewhite_1">
                    <View android:background="@color/black_900"
                        android:id="@+id/lineLine2"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_13pxv" android:layout_width="@dimen/_17pxh"/>
                    <View android:background="@color/black_900"
                        android:id="@+id/lineLine4"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_13pxh"/>
                    <View android:background="@color/black_900"
                        android:id="@+id/lineLine3"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_8pxh"/>
                </LinearLayout>
                <LinearLayout android:gravity="center"
                    android:id="@+id/linearGroup106"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_61pxh"
                    android:layout_weight="1.1"
                    android:layout_width="0dp" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearGroup107"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:gravity="center"
                            android:id="@+id/txtDeliveryto"
                            android:layout_gravity="top"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_34pxh"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:text="@{productpageVM.productpageModel.txtDeliveryto}"
                            style="@style/txtSkModernistskmodernistregular14_4" tools:text="@string/lbl_delivery_to"/>
                        <View android:background="@color/black_900"
                            android:id="@+id/lineVector2"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_3pxv"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginEnd="@dimen/_18pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_7pxh"/>
                    </LinearLayout>
                    <TextView android:gravity="center"
                        android:id="@+id/txtLekkiphase1"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:text="@{productpageVM.productpageModel.txtLekkiphase1}"
                        style="@style/txtDMSansregular15" tools:text="@string/msg_lekki_phase_1"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameRectangle13"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_60pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_40pxh" style="@style/groupStylewhite_1">
                    <ImageView android:id="@+id/imageImage2"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_23pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_image2"/>
                </FrameLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearGroup104"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_20pxv" android:paddingTop="@dimen/_40pxv">
                <TextView android:gravity="start"
                    android:id="@+id/txtEnjoyDelicious"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_width="match_parent"
                    android:text="@{productpageVM.productpageModel.txtEnjoyDelicious}"
                    style="@style/txtDMSansbold24_1" tools:text="@string/msg_enjoy_delicious"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerGroup461"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_50pxv"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_22pxh}" tools:itemCount="4" tools:listitem="@layout/row_productpage1"/>
                <LinearLayout android:id="@+id/linearGroup116"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_50pxv"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearGroup412"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:gravity="top|start"
                            android:id="@+id/txtPopularrestaur"
                            android:layout_gravity="top"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.7"
                            android:layout_width="0dp"
                            android:text="@{productpageVM.productpageModel.txtPopularrestaur}"
                            style="@style/txtDMSansbold16" tools:text="@string/msg_popular_restaur"/>
                        <TextView android:gravity="top|end"
                            android:id="@+id/txtViewall29"
                            android:layout_gravity="top"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_21pxv"
                            android:layout_marginEnd="@dimen/_96pxh"
                            android:layout_marginStart="@dimen/_110pxh"
                            android:layout_weight="0.4"
                            android:layout_width="0dp"
                            android:text="@{productpageVM.productpageModel.txtViewall29}"
                            style="@style/txtSkModernistskmodernistregular14_5" tools:text="@string/lbl_view_all_29"/>
                    </LinearLayout>
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recyclerGroup117"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_width="match_parent"
                        android:nestedScrollingEnabled="false"
                        android:orientation="horizontal"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        itemSpace="@{@dimen/_29pxh}" tools:itemCount="2" tools:listitem="@layout/row_productpage2"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_gravity="start"
            android:layout_height="wrap_content" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageRectangle26"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_115pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY" android:src="@drawable/img_rectangle26"/>
            <LinearLayout android:id="@+id/linearGroup34"
                android:layout_gravity="center|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_45pxv"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIconlyBulkHom"
                    android:layout_gravity="bottom"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_39pxh"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_iconlybulkhom"/>
                <ImageView android:id="@+id/imageIconlyLightHe"
                    android:layout_gravity="bottom"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_iconlylighthe_2"/>

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/floatingBtnFloatingactionbutton"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_width="wrap_content"
                    app:fabCustomSize="@dimen/_60pxh" app:srcCompat="@drawable/img_group33"/>
                <ImageView android:id="@+id/imageIconlyLightNo"
                    android:layout_gravity="bottom"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_iconlylightno"/>
                <ImageView android:id="@+id/imageBuy"
                    android:layout_gravity="bottom"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginEnd="@dimen/_42pxh"
                    android:layout_marginStart="@dimen/_37pxh"
                    android:layout_marginTop="@dimen/_37pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_buy"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
