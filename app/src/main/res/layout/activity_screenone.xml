<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="screenoneVM" type="com.ankitpatelsandroidapplication.app.modules.screenone.data.viewmodel.ScreenoneVM"/>
    </data>
    <LinearLayout android:id="@+id/linearGroup173"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearGroup171"
                android:layout_gravity="top|start"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIcon"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_171pxh"
                    android:layout_width="@dimen/_34pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon_1"/>
                <TextView android:gravity="top|end"
                    android:id="@+id/txtSkip"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_107pxh"
                    android:layout_weight="0.2"
                    android:layout_width="0dp"
                    android:text="@{screenoneVM.screenoneModel.txtSkip}"
                    style="@style/txtSkModernistskmodernistbold16" tools:text="@string/lbl_skip"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearGroup170"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_110pxv" android:paddingTop="@dimen/_49pxv">
                <TextView android:gravity="center"
                    android:id="@+id/txtOrderfromyour"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_48pxh"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_width="match_parent"
                    android:text="@{screenoneVM.screenoneModel.txtOrderfromyour}"
                    style="@style/txtDMSansbold24" tools:text="@string/msg_order_from_your"/>
                <ImageView android:id="@+id/imageGroup391"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_255pxh"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_46pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_group391}" tools:src="@drawable/img_group391"/>
                <ImageView android:id="@+id/imageGroup2"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_8pxh"
                    android:layout_marginEnd="@dimen/_163pxh"
                    android:layout_marginStart="@dimen/_164pxh"
                    android:layout_marginTop="@dimen/_45pxv"
                    android:layout_width="@dimen/_48pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_group2"/>
                <LinearLayout android:id="@+id/linearGroup458"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_60pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <FrameLayout android:clickable="true"
                        android:id="@+id/frameButton"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" style="@style/groupStylecornerRadius">
                        <TextView android:gravity="center"
                            android:id="@+id/txtCreateanaccou"
                            android:layout_gravity="center"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_15pxv"
                            android:layout_marginEnd="@dimen/_107pxh"
                            android:layout_marginStart="@dimen/_107pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_width="match_parent"
                            android:text="@{screenoneVM.screenoneModel.txtCreateanaccou}"
                            style="@style/txtSkModernistskmodernistbold14" tools:text="@string/msg_create_an_accou"/>
                    </FrameLayout>
                    <FrameLayout android:clickable="true"
                        android:id="@+id/frameButton1"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_width="match_parent" style="@style/groupStylecornerRadius_1">
                        <TextView android:gravity="center"
                            android:id="@+id/txtCreateanaccou1"
                            android:layout_gravity="center"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_15pxv"
                            android:layout_marginEnd="@dimen/_150pxh"
                            android:layout_marginStart="@dimen/_150pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_width="match_parent"
                            android:text="@{screenoneVM.screenoneModel.txtCreateanaccou1}"
                            style="@style/txtSkModernistskmodernistbold16_1" tools:text="@string/lbl_login"/>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
