<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="detailspageVM" type="com.ankitpatelsandroidapplication.app.modules.detailspage.data.viewmodel.DetailspageVM"/>
    </data>
    <LinearLayout android:id="@+id/linearGroup142"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearGroup126"
                android:layout_gravity="top|start"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:clickable="true"
                    android:id="@+id/frameRectangle12"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_40pxh" style="@style/groupStylewhite_1">
                    <ImageView android:id="@+id/imageVector3"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="@dimen/_8pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_vector3"/>
                </FrameLayout>
                <LinearLayout android:gravity="center"
                    android:id="@+id/linearGroup127"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_61pxh"
                    android:layout_weight="1.1"
                    android:layout_width="0dp" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearGroup128"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:gravity="center"
                            android:id="@+id/txtDeliveryto"
                            android:layout_gravity="top"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_34pxh"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:text="@{detailspageVM.detailspageModel.txtDeliveryto}"
                            style="@style/txtSkModernistskmodernistregular14_4" tools:text="@string/lbl_delivery_to"/>
                        <View android:background="@color/black_900"
                            android:id="@+id/lineVector2"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_3pxv"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginEnd="@dimen/_18pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_7pxh"/>
                    </LinearLayout>
                    <TextView android:gravity="center"
                        android:id="@+id/txtLekkiphase1"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:text="@{detailspageVM.detailspageModel.txtLekkiphase1}"
                        style="@style/txtDMSansregular15" tools:text="@string/msg_lekki_phase_1"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameRectangle13"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_60pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_40pxh" style="@style/groupStylewhite_1">
                    <ImageView android:id="@+id/imageImage2"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_23pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_image2"/>
                </FrameLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearGroup125"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_31pxv" android:paddingTop="@dimen/_39pxv">
                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/cardViewRectangle18"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_58pxh"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_width="match_parent"
                    app:cardBackgroundColor="@color/white_A700"
                    app:cardCornerRadius="@dimen/_20pxh"
                    app:cardElevation="@dimen/_1pxh"
                    app:cardUseCompatPadding="true" style="@style/groupStylewhite_A700cornerRadius">
                    <ImageView android:id="@+id/imageRectangle19"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_150pxh"
                        android:layout_marginBottom="@dimen/_46pxv"
                        android:layout_marginEnd="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_28pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle19}" tools:src="@drawable/img_rectangle19"/>
                </com.google.android.material.card.MaterialCardView>
                <LinearLayout android:id="@+id/linearGroup414"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_35pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylecornerRadius_6">
                    <TextView android:gravity="top|center"
                        android:id="@+id/txt"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginStart="@dimen/_156pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:text="@{detailspageVM.detailspageModel.txt}"
                        style="@style/txtSkModernistskmodernistbold14" tools:text="@string/lbl5"/>
                    <TextView android:gravity="center"
                        android:id="@+id/txt2"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:text="@{detailspageVM.detailspageModel.txt2}"
                        style="@style/txtSkModernistskmodernistbold14" tools:text="@string/lbl_2"/>
                    <TextView android:gravity="top|center"
                        android:id="@+id/txt1"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_154pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:text="@{detailspageVM.detailspageModel.txt1}"
                        style="@style/txtSkModernistskmodernistbold14" tools:text="@string/lbl6"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearGroup421"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_58pxh"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_marginTop="@dimen/_35pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="center"
                        android:id="@+id/txtBigboysCheese"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:text="@{detailspageVM.detailspageModel.txtBigboysCheese}"
                        style="@style/txtDMSansbold24_1" tools:text="@string/msg_big_boys_cheese"/>
                    <LinearLayout android:id="@+id/linearGroup419"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearGroup418"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.7"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageStar1"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_15pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_15pxh"
                                android:scaleType="fitXY" android:src="@drawable/img_star1_1"/>
                            <TextView android:gravity="top|start"
                                android:id="@+id/txt4"
                                android:layout_gravity="top"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.7"
                                android:layout_width="0dp"
                                android:text="@{detailspageVM.detailspageModel.txt4}"
                                style="@style/txtSkModernistskmodernistregular11" tools:text="@string/lbl_4"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearGroup417"
                            android:layout_gravity="top"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_30pxh"
                            android:layout_weight="0.8"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txt3"
                                android:layout_gravity="top"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.4"
                                android:layout_width="0dp"
                                android:text="@{detailspageVM.detailspageModel.txt3}"
                                style="@style/txtDMSansregular12_1" tools:text="@string/lbl7"/>
                            <TextView android:gravity="top|start"
                                android:id="@+id/txt300cal"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_weight="1.5"
                                android:layout_width="0dp"
                                android:text="@{detailspageVM.detailspageModel.txt300cal}"
                                style="@style/txtSkModernistskmodernistregular11" tools:text="@string/lbl_300cal"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearGroup416"
                            android:layout_gravity="top"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_30pxh"
                            android:layout_weight="1"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txt5"
                                android:layout_gravity="top"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.3"
                                android:layout_width="0dp"
                                android:text="@{detailspageVM.detailspageModel.txt5}"
                                style="@style/txtDMSansregular12_1" tools:text="@string/lbl8"/>
                            <TextView android:gravity="center"
                                android:id="@+id/txtTime"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_weight="1.2"
                                android:layout_width="0dp"
                                android:text="@{detailspageVM.detailspageModel.txtTime}"
                                style="@style/txtSkModernistskmodernistregular11" tools:text="@string/lbl_5_10min"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameGroup130"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_35pxv" android:layout_width="match_parent">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtMore"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_216pxh"
                        android:layout_marginStart="@dimen/_71pxh"
                        android:layout_marginTop="@dimen/_63pxv"
                        android:layout_width="wrap_content"
                        android:text="@{detailspageVM.detailspageModel.txtMore}"
                        style="@style/txtSkModernistskmodernistregular14_5" tools:text="@string/lbl_more"/>
                    <TextView android:gravity="start"
                        android:id="@+id/txtDescription"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:text="@{detailspageVM.detailspageModel.txtDescription}"
                        style="@style/txtSkModernistskmodernistregular14_1" tools:text="@string/msg_our_simple_cla"/>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearGroup132"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_35pxv"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtToppingsforyo"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:text="@{detailspageVM.detailspageModel.txtToppingsforyo}"
                        style="@style/txtDMSansmedium16" tools:text="@string/msg_toppings_for_yo"/>
                    <FrameLayout android:id="@+id/frameGroup134"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20pxv" android:layout_width="wrap_content">
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recyclerDetailspagelist"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_18pxv"
                            android:layout_width="match_parent"
                            android:nestedScrollingEnabled="false"
                            android:orientation="horizontal"
                            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                            itemSpace="@{@dimen/_20pxh}"
                            tools:itemCount="3" tools:listitem="@layout/row_detailspage1"/>
                        <FrameLayout android:clickable="true"
                            android:id="@+id/frameButton"
                            android:layout_gravity="center|start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_40pxh"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_width="match_parent" style="@style/groupStylecornerRadius">
                            <TextView android:gravity="center"
                                android:id="@+id/txtCreateanaccou"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_15pxv"
                                android:layout_marginEnd="@dimen/_131pxh"
                                android:layout_marginStart="@dimen/_131pxh"
                                android:layout_marginTop="@dimen/_15pxv"
                                android:layout_width="match_parent"
                                android:text="@{detailspageVM.detailspageModel.txtCreateanaccou}"
                                style="@style/txtSkModernistskmodernistbold14" tools:text="@string/lbl_add_to_cart"/>
                        </FrameLayout>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>
